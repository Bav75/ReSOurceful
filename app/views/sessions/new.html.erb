<%# log in form %>
<%= form_for(:session, url: login_path) do |f| %>
    <%= f.label :email %>
    <%= f.text_field :email%>

    <br>
    <br>

    <%= f.label :password %>
    <%= f.password_field :password%>

    <br>
    <br>

    <%= f.submit 'Log in' %>
<% end %>

<%# link to sign up for new users %>

<p>New to ReSOurceful? <%= link_to "Sign up here", new_user_path %> </p>